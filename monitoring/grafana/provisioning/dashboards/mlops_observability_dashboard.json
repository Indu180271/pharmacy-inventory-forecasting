{
  "title": "MLOps Observability (Pipeline + Training + Inference)",
  "uid": "mlops-observability",
  "schemaVersion": 36,
  "version": 1,
  "editable": true,
  "style": "dark",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  },
  "panels": [
    {
      "type": "row",
      "title": "Pipeline Monitoring",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Pipeline Success",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "pipeline_success_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "stat",
      "title": "Pipeline Failures",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "pipeline_failure_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "timeseries",
      "title": "Pipeline Step Duration (p90, seconds)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(pipeline_step_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p90 duration",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Rows Ingested (total)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "rate(rows_ingested_total[5m])",
          "legendFormat": "rows/s",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 5, "w": 12, "h": 8 }
    },

    {
      "type": "row",
      "title": "Model Training Monitoring",
      "gridPos": { "x": 0, "y": 9, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Training Success",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "training_success_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 0, "y": 10, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "stat",
      "title": "Training Failures",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "training_failure_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 6, "y": 10, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "timeseries",
      "title": "Training Duration (p95, seconds)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(training_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95 training duration",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 10, "w": 12, "h": 8 }
    },
    {
      "type": "table",
      "title": "MAPE per SKU",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "training_mape_gauge",
          "legendFormat": "{{sku}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 8 },
      "options": {
        "showHeader": true
      }
    },

    {
      "type": "row",
      "title": "Inference Monitoring",
      "gridPos": { "x": 0, "y": 22, "w": 24, "h": 1 },
      "collapsed": false,
      "panels": []
    },
    {
      "type": "stat",
      "title": "Inference Success",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "inference_success_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 0, "y": 23, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "stat",
      "title": "Inference Failures",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        { "expr": "inference_failure_total", "legendFormat": "", "refId": "A" }
      ],
      "gridPos": { "x": 6, "y": 23, "w": 6, "h": 4 },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center"
      }
    },
    {
      "type": "timeseries",
      "title": "Inference Latency (p90, ms)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.90, sum(rate(inference_latency_seconds_bucket[5m])) by (le)) * 1000",
          "legendFormat": "p90 latency",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 23, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests per Path (FastAPI)",
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count[5m])) by (path)",
          "legendFormat": "{{path}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 27, "w": 24, "h": 8 }
    }
  ]
}
